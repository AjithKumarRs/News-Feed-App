<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Generic"/>

    <style name="Generic.Container">
        <item name="android:padding">@dimen/generic_padding</item>
        <item name="android:layout_marginLeft">@dimen/generic_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/generic_margin_horizontal</item>
        <item name="android:layout_marginBottom">@dimen/generic_margin_vertical</item>
        <item name="android:layout_marginTop">@dimen/generic_margin_vertical</item>
        <item name="android:background">@drawable/ll_generic_container</item>
        <item name="android:elevation">@dimen/generic_elevation</item>
    </style>

    <style name="Generic.Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:layout_marginBottom">@dimen/generic_padding_small</item>
    </style>

</resources>